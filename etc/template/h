#ifndef ${id ${upcase $fn}}
#define ${id ${upcase $fn}}

#pragma interface "${id ${trim_dir ${trim_ext $fn}}}"

class ${id ${trim_dir ${trim_ext $fn}}}
{
public:
	${id ${trim_dir ${trim_ext $fn}}}();
	${id ${trim_dir ${trim_ext $fn}}}(const ${id ${trim_dir ${trim_ext $fn}}} &);
	${id ${trim_dir ${trim_ext $fn}}} &operator=(const ${id ${trim_dir ${trim_ext $fn}}} &);
	~${id ${trim_dir ${trim_ext $fn}}}();
};

#endif /* ${id ${upcase $fn}} */
